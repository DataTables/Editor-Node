    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "@types/node": "^8.0.8",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-typescript": "^3.1.7",
    "tslint": "^5.5.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.4"
  },
  "importSort": {
